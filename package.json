{
  "name": "vscode-keybindings-snippets",
  "displayName": "VSCode keybindings/snippets",
  "description": "Useful keybindings/snippets for multiple languages, packages, and frameworks.",
  "version": "1.0.1",
  "license": "MIT",
  "scripts": {
    "build": "vsce package"
  },
  "repository": {
    "url": "https://github.com/bmunoz89/vscode-keybindings-snippets"
  },
  "publisher": "bmunoz89",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Snippets",
    "Keymaps"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/javascript.code-snippets"
      },
      {
        "language": "typescript",
        "path": "./snippets/javascript.code-snippets"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/javascript.code-snippets"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/javascript.code-snippets"
      },
      {
        "language": "typescript",
        "path": "./snippets/typescript.code-snippets"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/typescript.code-snippets"
      },
      {
        "language": "javascript",
        "path": "./snippets/sentry.code-snippets"
      },
      {
        "language": "typescript",
        "path": "./snippets/sentry.code-snippets"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/sentry.code-snippets"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/sentry.code-snippets"
      },
      {
        "language": "vue",
        "path": "./snippets/vue.code-snippets"
      },
      {
        "language": "javascript",
        "path": "./snippets/vuex.code-snippets"
      },
      {
        "language": "typescript",
        "path": "./snippets/vuex.code-snippets"
      }
    ],
    "keybindings": [
      {
        "key": "ctrl+cmd+j",
        "command": "extension.prettifyJSON"
      },
      {
        "key": "cmd+k j",
        "command": "workbench.action.togglePanel"
      },
      {
        "key": "ctrl+cmd+p",
        "command": "projectManager.listProjects"
      },
      {
        "key": "cmd+0",
        "command": "editor.action.fontZoomReset"
      },
      {
        "key": "cmd+=",
        "command": "editor.action.fontZoomIn"
      },
      {
        "key": "cmd+-",
        "command": "editor.action.fontZoomOut"
      },
      {
        "key": "cmd+k cmd+r",
        "command": "toggle",
        "args": {
          "id": "zen",
          "value": [
            {
              "typescript.implementationsCodeLens.enabled": true,
              "typescript.referencesCodeLens.enabled": true,
              "codemetrics.basics.CodeLensEnabled": true,
              "editor.renderWhitespace": "all",
              "gitlens.codeLens.recentChange.enabled": true,
              "gitlens.codeLens.authors.enabled": true
            },
            {
              "typescript.implementationsCodeLens.enabled": false,
              "typescript.referencesCodeLens.enabled": false,
              "codemetrics.basics.CodeLensEnabled": false,
              "editor.renderWhitespace": "selection",
              "gitlens.codeLens.recentChange.enabled": false,
              "gitlens.codeLens.authors.enabled": false
            }
          ]
        }
      },
      {
        "key": "cmd+k cmd+h",
        "command": "toggle",
        "args": {
          "id": "inlineHints",
          "value": [
            {
              "typescript.inlayHints.enumMemberValues.enabled": true,
              "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
              "typescript.inlayHints.parameterTypes.enabled": true,
              "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
              "typescript.inlayHints.variableTypes.enabled": true,
              "vue.inlayHints.optionsWrapper": true
            },
            {
              "typescript.inlayHints.enumMemberValues.enabled": false,
              "typescript.inlayHints.functionLikeReturnTypes.enabled": false,
              "typescript.inlayHints.parameterTypes.enabled": false,
              "typescript.inlayHints.propertyDeclarationTypes.enabled": false,
              "typescript.inlayHints.variableTypes.enabled": false,
              "vue.inlayHints.optionsWrapper": false
            }
          ]
        }
      },
      {
        "key": "cmd+k cmd+c",
        "command": "workbench.action.terminal.clear",
        "when": "terminalFocus"
      },
      {
        "key": "cmd+d",
        "command": "workbench.action.terminal.split",
        "when": "terminalFocus"
      },
      {
        "key": "cmd+w",
        "command": "workbench.action.terminal.kill",
        "when": "terminalFocus"
      },
      {
        "key": "alt+cmd+`",
        "command": "workbench.action.terminal.focusNextPane",
        "when": "terminalFocus"
      },
      {
        "key": "alt+cmd+[BracketRight]",
        "command": "workbench.action.terminal.focusPreviousPane",
        "when": "terminalFocus"
      },
      {
        "key": "cmd+n",
        "command": "extension.advancedNewFile"
      },
      {
        "key": "cmd+]",
        "command": "workbench.action.nextEditor",
        "when": "editorFocus"
      },
      {
        "key": "cmd+[",
        "command": "workbench.action.previousEditor",
        "when": "editorFocus"
      }
    ]
  }
}
